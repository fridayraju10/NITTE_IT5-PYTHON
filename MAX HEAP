import heapq

heap = []

elements = [20, 15, 30, 10, 1, 40, 2, 18]

for element in elements:
    heapq.heappush(heap, -element)

max_heap = [-i for i in heap]
print("Max Heap (simulated using min heap):", max_heap)

max_element = -heapq.heappop(heap)
print("Popped Max Element:", max_element)

remaining_heap = [-i for i in heap]
print("Heap after pop:", remaining_heap)
