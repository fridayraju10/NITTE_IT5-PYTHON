def gcd(a, b):
    while b != 0:
        a, b = b, a % b
    return a

def are_coprime(a, b):
    return gcd(a, b) == 1

range_limit = int(input("Enter the range: "))

for i in range(1, range_limit + 1):
    for j in range(1, i):
        for k in range(1, j):
            if k * k + j * j == i * i and are_coprime(i, j) and are_coprime(i, k) and are_coprime(j, k):
                print(i, j, k)
