a={1:3,2:'2','1':1, 2:'4'}
b=[1,2,3,4]
c={2,2,4,4}
for i in range(len(c)):
    b[i]=b[i]+i 
a[1]=b[1]
a['1']=a[1]
a['2']=b[2]
a[2]=a['1']
for i in a.keys():
    print(i,end=" 2 ")
