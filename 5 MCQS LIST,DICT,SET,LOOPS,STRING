word = "HKNUDT"

for i in range(len(word)):
    for j in range(i + 1):
        print(chr(ord(word[j]) + 1), end="")
    print()
-----------------------------------------------------
c = 0
for i in range(3):
    for j in range(4):
        if j == i:
            break
        c += 1
        for k in range(5):
            if k == j:
                break
            c += 1

print(c)
