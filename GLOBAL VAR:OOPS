a = 3
class CSE:
    def boys(self):
        global a
        for i in range(3):
            a=a+i
            print(a,end=" ")
        print()

class ECE:
    def girls(self):
        global a
        a=0
        for i in range(4,7):
            a=a+i
            print(a,end=" ")
            


b = CSE()
g = ECE()

b.boys()   
g.girls()  
